<phpdox xmlns="http://phpdox.de/config">
  <project name="OpenEMR" source="${basedir}" workdir="build/phpdox">
    <collector publiconly="false">
      <include mask="*.php" />
    </collector>

    <generator output="build">
      <build engine="html" enabled="true" output="api">
        <file extension="html" />
      </build>
    </generator>
  </project>
</phpdox>
